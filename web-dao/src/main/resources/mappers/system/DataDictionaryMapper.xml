<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sneaklife.dao.system.dictionary.DataDictionaryMapper">
  <!-- 数据插入 -->
  <insert id="insertDataDictionary" parameterType="map">
    insert into data_dictionary(
      id,
      <if test="name != null">
        name,
      </if>
      <if test="type">
        type,
      </if>
      <if test="value != null">
        value,
      </if>
      create_date,
      update_date,
      is_del
    ) values (
      REPLACE(UUID(), '-', ''),
      <if test="name != null">
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="type">
        #{type,jdbcType=VARCHAR},
      </if>
      <if test="value != null">
        #{value,jdbcType=VARCHAR},
      </if>
      NOW(),
      NOW(),
      0
    )
  </insert>
</mapper>
