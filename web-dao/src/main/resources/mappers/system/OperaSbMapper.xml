<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sneaklife.dao.system.opera.OperaSbMapper">
  <select id="findOperaSbByShow" resultType="com.sneaklife.dao.entity.OperaSb" parameterType="map">
